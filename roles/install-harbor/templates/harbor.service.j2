[Unit]
Description=Harbor
After=docker.service systemd-networkd.service systemd-resolved.service
Requires=docker.service

[Service]
Type=simple
Restart=on-failure
RestartSec=5
ExecStart=/usr/local/bin/docker-compose -f /data01/harbor/docker-compose.yml up
ExecStop=/usr/local/bin/docker-compose -f /data01/harbor/docker-compose.yml down
[Install]
WantedBy=multi-user.target