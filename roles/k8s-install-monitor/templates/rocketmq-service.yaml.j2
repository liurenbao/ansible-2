apiVersion: v1
kind: Endpoints
metadata:
  name: rocketmq
  namespace: monitoring
subsets:
  - addresses:
      - ip: 192.168.160.72
    ports:
      - port: 9121
---
apiVersion: v1
kind: Service
metadata:
  name: rocketmq
  namespace: monitoring
  annotations:
    prometheus.io/scrape: "true"
    prometheus.io/port: '9121'
    prometheus.io/scheme: "http"
spec:
  ports:
    - port: 9121