apiVersion: v1
kind: Endpoints
metadata:
  name: rocketmq
  namespace: monitoring
subsets:
  - addresses:
      - ip: '{{ groups["rocketmq-console"][0] }}'
    ports:
      - port: 9876
---
apiVersion: v1
kind: Service
metadata:
  name: rocketmq
  namespace: monitoring
  annotations:
    prometheus.io/tcp-probe: "true"
    prometheus.io/scrape: "true"
    prometheus.io/port: '9121'
    prometheus.io/scheme: "http"
spec:
  ports:
    - port: 9876