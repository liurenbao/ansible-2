apiVersion: v1
kind: Endpoints
metadata:
  name: elasticsearch
  namespace: monitoring
subsets:
  - addresses:
      - ip: 192.168.161.200
      - ip: 192.168.161.196
      - ip: 192.168.161.193
    ports:
      - port: 9114
---
apiVersion: v1
kind: Service
metadata:
  name: elasticsearch
  namespace: monitoring
  annotations:
    prometheus.io/scrape: "true"
    prometheus.io/port: '9114'
    prometheus.io/scheme: "http"
spec:
  ports:
    - port: 9114