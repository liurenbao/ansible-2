apiVersion: v1
kind: Endpoints
metadata:
  name: ravencast
  namespace: monitoring
subsets:
  - addresses:
      - ip: 192.168.161.208
      - ip: 192.168.161.209
    ports:
      - port: 9998
---
apiVersion: v1
kind: Service
metadata:
  name: ravencast
  namespace: monitoring
  annotations:
    prometheus.io/scrape: "true"
    prometheus.io/port: '9998'
    prometheus.io/scheme: "http"
    prometheus.io/path: '/'
spec:
  ports:
    - port: 9998
  type: ClusterIP
